{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".int-spec.ts$",
  "transform": {
    "^.+\\.(t)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@/(.*)": "<rootDir>/src/$1"
  },
  "setupFilesAfterEnv": ["<rootDir>/test/globalMocks.ts"]
}
